<view style="margin-top:10px; margin-left:8%;" class='images_view'>
  <view wx:for="{{images}}" wx:for-item="itemss" wx:for-index="i" >

    <block wx:for="{{itemss}}" wx:for-item="items" wx:for-index="j" >
      <image class='images' style="width:35px;height:35px;margin-left:0px;" src="{{items}}" bindtap="bindImageBlur" id="{{i}},{{j}}"> </image>
      <!-- <icon type="success" size="40" /> -->
      <!-- <block class="flex-item"  style="margin-left:5px;" > {{i}},{{j}}={{items}} </block> -->
    </block>


  </view>
</view>


<button class='but_start' style="height:40px; margin-top:10px;" bindtap="bindStartBlur"> {{butVal}} </button>
