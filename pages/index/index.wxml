


<!-- 行列数 -->
<view style="margin-left:8%; margin-top:10px; font-size:15px;" >行列数:</view>
<view class='select_box'  style='font-size:15px;' >
  <view class='select' catchtap='selectTapR'>
    行:
    <text class='select_text'>{{selectData[index_r]}}</text>
    <image class='select_img {{show_r&&"select_img_rotate"}}' src='../image/dd.png'></image>
  </view>

  <view class='option_box' style='height:{{show_r?(selectData.length*50):0}}rpx; margin-top:30px;'>
    <text class='option' style='{{index==selectData.length-1&&"border:0;"}}' wx:for='{{selectData}}' wx:key='this' data-index='{{index}}' catchtap='optionTapR'>{{item}}</text>
  </view>


  <view class='select' catchtap='selectTapC' style="margin-left:50%;">
    列:
    <text class='select_text'>{{selectData[index_c]}}</text>
    <image class='select_img {{show_c&&"select_img_rotate"}}' src='../image/dd.png'></image>
  </view>

  <view class='option_box' style='height:{{show_c?(selectData.length*50):0}}rpx; margin-left:50%; margin-top:30px;'>
    <text class='option' style='{{index==selectData.length-1&&"border:0;"}}' wx:for='{{selectData}}' wx:key='this' data-index='{{index}}' catchtap='optionTapC'>{{item}}</text>
  </view>


</view>

